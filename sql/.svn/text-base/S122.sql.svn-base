select dzeitsoll.persnr,dpers.name,dpers.vorname,dzeitsoll.oe,calendar.datum,calendar.cislodne, dzeitsoll.stunden as stunden from calendar left join dzeitsoll on calendar.datum = dzeitsoll.datum left join dzeit on calendar.datum = dzeit.datum join dpers on dpers.persnr=dzeitsoll.persnr where ((calendar.datum between '2009-08-01' and '2009-08-31') and (dzeitsoll.persnr between '104' and '104') and (dzeitsoll.oe <>'-')) group by dzeitsoll.persnr,oe,calendar.datum